        $(".main-wrapper").removeClass("blurred");
    });

    $(".accordion-item").click(function () {
        $(this).find(".accordion-open").toggleClass('openedup');
        $(this).find(".accordion-open").slideToggle();
        $(this).find(".accordion-open").not(this).removeClass('openedup');
        $(this).find(".accordion-open").not('.openedup').slideUp();
    });
});
</script>
